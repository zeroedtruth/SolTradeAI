{
  "name": "monetai-backend",
  "version": "1.0.0",
  "description": "MonetAI API",
  "author": "MonadAI",
  "license": "ISC",
  "scripts": {
    "start:dev": "docker compose --env-file .env up -d  && cross-env NODE_ENV=development tsx watch --clear-screen=false src/server.ts ",
    "start:local": "docker compose --env-file .env up -d  && cross-env NODE_ENV=local  tsx watch --clear-screen=false src/server.ts",
    "start:test": "cross-env NODE_ENV=test  tsx watch --clear-screen=false src/server.ts ",
    "build": "pwd && ls && cd node_modules/@types/glob && npm i @types/minimatch@latest && cd ../../../ && set NODE_OPTIONS=--max_old_space_size=2048 && tsc --project tsconfig.json && tsconfig-replace-paths --project tsconfig.json -s ./src -o ./build && mkdir -p ./build/templates && cp package.json ./build/ && cp .sequelizerc ./build/ && cp -r ./src/templates/* ./build/templates/ && mkdir -p ./build/database/constants && cp -r ./src/database/constants/*.csv ./build/database/constants/ && cd ./build && npm i --omit=dev",
    "build:local": "set NODE_OPTIONS=--max_old_space_size=2048 && tsc --project tsconfig.json && tsconfig-replace-paths -v --project tsconfig.json -s ./src -o ./build",
    "test:local": "cross-env NODE_ENV=testing jest  --config jest.config.ts --forceExit --detectOpenHandles  --testPathPattern=src/tests",
    "test:dev": "cross-env NODE_ENV=development jest --forceExit --detectOpenHandles --watchAll --testPathPattern=src/tests",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "db:migrate:one": "npx sequelize-cli db:migrate --name=$1",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo --name=$1",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:make:migrations": "./node_modules/sequelize-auto-migrations/bin/makemigration.js",
    "db:seed:all": "npx sequelize db:seed:all",
    "db:seed:undo:all": "npx sequelize db:seed:undo:all",
    "migration:run": "sequelize-cli db:migrate",
    "migration:undo": "sequelize-cli db:migrate:undo",
    "migration:undo:all": "sequelize-cli db:migrate:undo:all"
  },
  "engines": {
    "node": ">=18.1"
  },
  "jest": {
    "testTimeout": 9000,
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "testEnvironment": "node"
  },
  "jest-junit": {
    "outputDirectory": "test_reports",
    "outputName": "jest-junit.xml"
  },
  "dependencies": {
    "@bull-board/api": "^6.7.10",
    "@bull-board/express": "^6.7.10",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.0",
    "body-parser": "^1.20.3",
    "bull": "^4.16.5",
    "bullmq": "^5.41.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "class-validator-jsonschema": "^5.0.1",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "envalid": "^8.0.0",
    "ethers": "^5.7.2",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-basic-auth": "^1.2.1",
    "express-ws": "^5.0.2",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "openai": "^4.85.4",
    "openapi-types": "^12.1.3",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "response-time": "^2.3.3",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.5",
    "twitter-api-v2": "^1.20.2",
    "uuid-parse": "^1.1.0",
    "uuidv4": "^6.2.13",
    "viem": "^2.23.6",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@faker-js/faker": "^9.3.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.4",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.12.2",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "faker": "^6.6.6",
    "husky": "^9.1.7",
    "jest-extended": "^4.0.2",
    "prettier": "^3.4.2",
    "sequelize-auto": "^0.8.8",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-replace-paths": "^0.0.14",
    "tsx": "^4.19.2",
    "typescript": "5.7.2"
  }
}
